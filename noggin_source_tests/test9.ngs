declare function void sw(char hello);
declare function void sw1(char hello);
declare function void doSomething();
declare function void doAnotherThing();
declare function void lol();

# test switch statement
function void sw(char hello) {
  switch(hello) {
  case 'a':
    doSomething();
    fallthrough;
  case 'b':
    doAnotherThing();
  default:
    lol();
  }
}

function void sw1(char hello) {
  switch(hello) {
  case 'a':
    doSomething();
    fallthrough;
  case 'b':
    doAnotherThing();
  }
}
